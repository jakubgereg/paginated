{
  "name": "@jgereg/paginated",
  "version": "0.0.1",
  "description": "Paginate arrays with specified page number and size, returning items and pagination details.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakubgereg/paginated.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/**.*"
  ],
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write '**/*.{ts,tsx,js,html}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,html}'",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "check": "npm run format:check && npm run typecheck && npm run test",
    "preversion": "npm run check",
    "postversion": "git push && git push --tags && npm publish --access public",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "pagination",
    "paginate",
    "array pagination",
    "paginated array",
    "page size",
    "page number",
    "pagination utility",
    "data pagination",
    "pagination helper",
    "TypeScript pagination",
    "paginate function",
    "array slicing",
    "pagination module",
    "paginated results",
    "page navigation",
    "frontend pagination"
  ],
  "author": "Jakub Gereg",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "29.5.5",
    "jest": "29.7.0",
    "prettier": "3.2.4",
    "ts-node": "10.9.2",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/jakubgereg/paginated/issues"
  },
  "homepage": "https://github.com/jakubgereg/paginated#readme"
}
